{
    "multipart": [
        {   "when": { "OR": [
                {"north": false, "east": false, "south": false, "west": false},
                {"north": true, "east": true },
                {"east": true, "south": true },
                {"south": true, "west": true},
                {"west": true, "north": true}
            ]},
            "apply": { "model": "witchcraftmod:block/chalk_dot" }
        },
        {   "when": { "OR": [
                { "north": true },
                { "north": false, "east": false, "south": true, "west": false }
            ]},
            "apply": { "model": "witchcraftmod:block/chalk_side" }
        },
        {   "when": { "OR": [
                { "south": true },
                { "north": true, "east": false, "south": false, "west": false }
            ]},
            "apply": { "model": "witchcraftmod:block/chalk_side_alt" }
        },
        {   "when": { "OR": [
                { "east": true },
                { "north": false, "east": false, "south": false, "west": true }
            ]},
            "apply": { "model": "witchcraftmod:block/chalk_side_alt", "y": 270 }
        },
        {   "when": { "OR": [
                { "west": true },
                { "north": false, "east": true, "south": false, "west": false }
            ]},
            "apply": { "model": "witchcraftmod:block/chalk_side", "y": 270 }
        }
    ]
}
